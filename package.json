    "protobuf",
    "glue",
    "google",
    "proto",
    "aws",
    "schema",
    "converter",
    "proto-to-glue",
    "protocol-buffers",
    "aws-glue",
    "glue-catalog",
    "schema-conversion",
    "data-lake",
    "etl",
    "aws-cdk",
    "data-engineering",
    "schema-generation",
    "nested-types",
    "type-mapping",
    "data-processing",
    "cloud-etl",
    "aws-data-lake",
    "schema-management",
    "data-warehousing",
    "data-integration",
    "protobuf-to-glue",
    "schema-migration",
    "data-transformation"
  ],
  "author": "Shubham Shah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shubhamshah207/proto-to-glue/issues"
